node{
  stage('git checkout'){
    git credentialsId: 'GITCredential', url: 'https://github.com/RSI1982/netcore-boilerplate.git'
  }
   stage('project build'){
    sh label: '', script: 'dotnet build HappyCode.NetCoreBoilerplate.sln'
  }
  stage('artifactory store'){
    sh label: '', script: 'cd ${WORKSPACE}/src/HappyCode.NetCoreBoilerplate.Api/bin/Debug/netcoreapp3.1'
    sh label: '', script: 'cp ${WORKSPACE}/src/HappyCode.NetCoreBoilerplate.Api/bin/Debug/netcoreapp3.1 http://104.47.146.170:8081/repository/nuget.org-proxy/'
  }
}
